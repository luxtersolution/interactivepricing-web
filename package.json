{
  "name": "interactivepricing-web",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "config": {
    "protopath": "./proto/"
  },
  "scripts": {
    "start": "node index.js",
    "start:watch": "node index.js",
    "build": "webpack --progress",
    "build:watch": "webpack --progress -w",
    "build:prod": "NODE_ENV=production webpack --progress",
    "postinstall": "mkdir ./server/proto && find $npm_package_config_protopath -name \"*.proto\" -exec protoc --plugin=protoc-gen-grpc=/usr/local/bin/grpc_node_plugin --grpc_out=./server/proto/ --proto_path=$npm_package_config_protopath --js_out=\"import_style=commonjs,binary:./server/proto/\" {} +"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.10",
    "anchor-js": "^4.1.0",
    "angular": "^1.6.6",
    "angular-ui-bootstrap": "^2.5.6",
    "bluebird": "^3.5.1",
    "body-parser": "~1.18.2",
    "bootstrap": "^4.0.0-beta.2",
    "copy-webpack-plugin": "^4.2.0",
    "dotenv": "^4.0.0",
    "expose-loader": "^0.7.3",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-validator": "^4.2.1",
    "file-loader": "^1.1.5",
    "google-protobuf": "^3.4.0",
    "grpc": "^1.6.6",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.1.0",
    "julian": "^0.2.0",
    "moment": "^2.19.1",
    "morgan": "~1.9.0",
    "normalize.css": "^7.0.0",
    "popper.js": "^1.12.6",
    "pug": "2.0.0-beta11",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "debug": "^2.6.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp": "^3.9.1",
    "gulp-exec": "^2.1.3",
    "html-loader": "^0.5.1",
    "karma": "^1.7.1",
    "ng-annotate-loader": "^0.6.1",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.8",
    "precss": "^2.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  }
}
